{
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "esnext",
    "paths": {
      "@dominicstop/utils": [
        "../../src"
      ],
    },
  },
  "include": [
    "src",
    "App.js",
    "index.js",
  ],
  "exclude": [
    "../../../node_modules",
    "../../node_modules",
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
}
